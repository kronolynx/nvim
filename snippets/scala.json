{
  "pln": {
    "prefix": "pln",
    "body": [
      "println(\"<<<>>>  $1\")",
      "$0"
    ],
    "description": "Log output to console"
  },
  "ppl": {
    "prefix": "ppl",
    "body": [
      "pprint.log(${1:EXPR}, tag = \"${1:EXPR}\")"
